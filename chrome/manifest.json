{
  "name": "Trello Clone",
  "version": "0.3.1",
  "description": "Clone a Trello Board",
  "icons": { "128": "images/128-icon.png" },
  "homepage_url": "https://github.com/oo7ph/Trello-Clone",
  "content_scripts": [
    {
		"matches": ["https://trello.com/board/*"],
		"js": [
			"inject/jquery.livequery.js",
			"inject/trello_clone_button.js",
			"inject/trello_clone_board.js",
			"inject/trello_clone_card.js",
			"inject/trello_clone_list.js",
			"injectScript.js",
			"injectManifest.js"
		]
	}
  ]
}
